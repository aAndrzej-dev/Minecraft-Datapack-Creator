<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>MinecraftDatapackCreator</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>Minecraft Datapack Creator</AssemblyName>
    <Company>aAndrzej-dev</Company>
    <Authors>aAndrzej-dev</Authors>
    <Copyright>Copyright © aAndrzej-dev 2022</Copyright>
    <NeutralLanguage>en</NeutralLanguage>
    <StartupObject>MinecraftDatapackCreator.Program</StartupObject>
    <ProduceReferenceAssembly>True</ProduceReferenceAssembly>
    <PlatformTarget>AnyCPU</PlatformTarget>
	  <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <DebugType>none</DebugType>
    <PackageProjectUrl>http://datapack.aandrzej.com/creator/</PackageProjectUrl>
	<RepositoryUrl>https://github.com/aAndrzej-dev/Minecraft-Datapack-Creator</RepositoryUrl>
    <ApplicationIcon>Icon.ico</ApplicationIcon>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
	  <Version>1.0.0-beta1</Version>
    <SupportedOSPlatformVersion>7.0</SupportedOSPlatformVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <PlatformTarget>x64</PlatformTarget>
  </PropertyGroup>



  <ItemGroup>
    <PackageReference Include="Aadev.JTF.Editor" Version="1.0.0-rc1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Aadev.JTF" Version="1.0.1" />
  </ItemGroup>


  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>