<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <RootNamespace>MinecraftDatapackCreator</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>true</UseWPF>
    <AssemblyName>MinecraftDatapackCreator</AssemblyName>
    <Company>aAndrzej-dev</Company>
    <Authors>aAndrzej-dev</Authors>
    <Copyright>Copyright © aAndrzej-dev 2025</Copyright>
	<ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<DebugType>none</DebugType>
    <PackageProjectUrl>http://datapack.aandrzej.com/creator/</PackageProjectUrl>
	<RepositoryUrl>https://github.com/aAndrzej-dev/Minecraft-Datapack-Creator</RepositoryUrl>
    <ApplicationIcon>Icon.ico</ApplicationIcon>
    <AssemblyVersion>1.0.0.5</AssemblyVersion>
    <FileVersion>1.0.0.5</FileVersion>
	<Version>1.0.0-beta5</Version>
    <SupportedOSPlatformVersion>7.0</SupportedOSPlatformVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <NeutralLanguage>en</NeutralLanguage>
    <AnalysisLevel>latest-all</AnalysisLevel>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Product>Minecraft Datapack Creator</Product>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>




	<ItemGroup>
		<PackageReference Include="Aadev.JTF" Version="2.0.0-beta6" />
		<PackageReference Include="Aadev.ConditionsInterpreter" Version="1.0.1" />
		<PackageReference Include="Aadev.JTF.Editor" Version="2.0.0-beta5" />
		<PackageReference Include="Aadev.NBT" Version="1.3.10" />
		<PackageReference Include="CommunityToolkit.Diagnostics" Version="8.4.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>




  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.pl-PL.resx">
      <Generator></Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>